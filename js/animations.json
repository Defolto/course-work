        "skewAnim": ["<br>@keyframes skewAnim{<br>&emsp;0%{<br>&emsp;&emsp;transform: scale(.1);<br>&emsp;}<br><br>&emsp;100%{<br>&emsp;&emsp;transform: scale(1);<br>&emsp;}<br>}"],

